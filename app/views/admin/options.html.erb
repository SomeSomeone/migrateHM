
<div class="wrapper wrapper-content animated fadeInRight">
<div class="row">
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
    <h5>Все адмиские настройки</h5>
    <div class="ibox-tools">
        <a class="collapse-link">
            <i class="fa fa-chevron-up"></i>
        </a>
    </div>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered table-hover dataTables-example">
<thead>
<tr>
    <th>id</th>
    <th>Имя</th>
    <th>Значение</th>
    <th>Обновить</th>
</tr>
</thead>
<tbody>
<% @admin_options.each do | admin_option | %>
<tr class="gradeX">
    <td><%=admin_option.id %></td>
    <th><%=admin_option.key %></th>
    <th><%=admin_option.value %></th>
    <td><%=link_to 'Обновить',admin_admin_option_edit_path(admin_option) %></td>
</tr>
<% end %>
</tbody>
<tfoot>
<tr>
    <th>id</th>
    <th>Имя</th>
    <th>Значение</th>
    <th>Обновить</th>
</tr>
</tfoot>
</table>
</div>
</div>
</div>
</div>

</div>
