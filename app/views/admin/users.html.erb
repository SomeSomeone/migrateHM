    <th><%=user.email %></th>
    <th><%=user.tel %></th>
</tr>
<% end %>
</tbody>
<tfoot>
<tr>
    <th>id</th>
    <th>Имя</th>
    <th>Почта</th>
    <th>Номер</th>
</tr>
</tfoot>
